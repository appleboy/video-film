<% _.each(videos, function (item) { %>
<div class="col-md-2 video">
  <div class="thumbnail">
    <a class="images" href="<%= item.video.nba_id %>" style="background-image: url('<%= item.video.thumbnail %>')">
      <span class="duration"><%= item.video.duration %></span>
    </a>
    <div class="caption">
      <h3 class="title"><%= item.video.title %></h3>
      <p class="date"><span class="label label-default"><%= item.video.game_date() %></span></p>
      <p><a href="<%= item.video.src %>" class="btn btn-primary btn-sm" role="button"><span class="glyphicon glyphicon-download-alt" aria-hidden="true"></span> 下載</a> <a href="#" class="btn btn-success btn-sm" role="button"><span class="glyphicon glyphicon-facetime-video" aria-hidden="true"></span> 觀看</a></p>
    </div>
  </div>
</div>
<% }) %>
